{
  "name": "htmlCssEditor",
  "version": "1.0.0",
  "description": "HTML/CSS File Editor",
  "main": "main.js",
  "scripts": {
    "webpack-watch": "./node_modules/webpack/bin/webpack.js --watch --progress --colors -d",
    "webpack-build": "./node_modules/webpack/bin/webpack.js --progress --colors -d",
    "webpack-build-test": "NODE_ENV=test ./node_modules/webpack/bin/webpack.js --progress --colors -d",
    "webpack-build-prod": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --progress --colors -p",
    "test": "NODE_ENV=test ./node_modules/.bin/karma start",
    "postinstall": "npm run webpack-build"
  },
  "author": "Wakanda SAS",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "clean-webpack-plugin": "^0.1.3",
    "css-loader": "^0.16.0",
    "ejs": "^2.3.3",
    "file-loader": "^0.8.4",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.11.0"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-mocha": "^0.2.0",
    "mocha": "^2.2.5"
  }
}
